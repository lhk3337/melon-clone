mixin toplist(item, index)
    li.list
        div.leftContainer
            div.rank
                span=`${index+1}`
            div.thumbContainer
                img(class="thumbAlbum" src=`${item.thumbUrl}`, alt="thumbAlbum")
            div.albuminfo
                span=`${item.title}`
                span=`${item.artist}`
        div.rightContainer
            div
                h4.listnerTitle Listners
                div.listnercount=`${item.playcount===undefined?0:item.playcount}`
            button.smMoreBtn(data-index=`${index}`)
                svg(xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="currentColor" class="bi bi-three-dots-vertical" viewBox="0 0 16 16")
                    path(d="M9.5 13a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0zm0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0zm0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0z")
            div.smContainer(data-index=`${index}`)
                button(class=["listenBtn","smBtn"] data-index=`${index}` data-mid=`${item._id}` data-id=`${item.ytUrl}`)
                    svg(xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="currentColor" class="bi bi-music-note" viewBox="0 0 16 16")
                        path(d="M9 13c0 1.105-1.12 2-2.5 2S4 14.105 4 13s1.12-2 2.5-2 2.5.895 2.5 2z")
                        path(fill-rule="evenodd" d="M9 3v10H8V3h1z")
                        path(d="M8 2.82a1 1 0 0 1 .804-.98l3-.6A1 1 0 0 1 13 2.22V4L8 5V2.82z")
                button(class=["addBtn","smBtn"]  data-id=`${item._id}`)
                    svg(xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="currentColor" class="bi bi-folder-plus" viewBox="0 0 16 16")
                        path(d="m.5 3 .04.87a1.99 1.99 0 0 0-.342 1.311l.637 7A2 2 0 0 0 2.826 14H9v-1H2.826a1 1 0 0 1-.995-.91l-.637-7A1 1 0 0 1 2.19 4h11.62a1 1 0 0 1 .996 1.09L14.54 8h1.005l.256-2.819A2 2 0 0 0 13.81 3H9.828a2 2 0 0 1-1.414-.586l-.828-.828A2 2 0 0 0 6.172 1H2.5a2 2 0 0 0-2 2Zm5.672-1a1 1 0 0 1 .707.293L7.586 3H2.19c-.24 0-.47.042-.683.12L1.5 2.98a1 1 0 0 1 1-.98h3.672Z")
                        path(d="M13.5 9a.5.5 0 0 1 .5.5V11h1.5a.5.5 0 1 1 0 1H14v1.5a.5.5 0 1 1-1 0V12h-1.5a.5.5 0 0 1 0-1H13V9.5a.5.5 0 0 1 .5-.5Z")
            button(class=["listenBtn","btn"] data-mid=`${item._id}` data-id=`${item.ytUrl}`)
                svg(xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="currentColor" class="bi bi-music-note" viewBox="0 0 16 16")
                    path(d="M9 13c0 1.105-1.12 2-2.5 2S4 14.105 4 13s1.12-2 2.5-2 2.5.895 2.5 2z")
                    path(fill-rule="evenodd" d="M9 3v10H8V3h1z")
                    path(d="M8 2.82a1 1 0 0 1 .804-.98l3-.6A1 1 0 0 1 13 2.22V4L8 5V2.82z")
            button(class=["addBtn","btn"]  data-id=`${item._id}`)
